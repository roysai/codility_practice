    "def solution (A, B, K):\n",
    "    if A%K==0:\n",
    "        first_num=A\n",
    "    else:\n",
    "        first_num=A+(K-A%K)\n",
    "    last_num=B-B%K\n",
    "    print (first_num, last_num)\n",
    "    count=int((last_num-first_num)/K)+1\n",
    "    return count"
   ]
