   "source": [
    "def rotate(A, n):\n",
    "    n=n%len(A)\n",
    "    remain=A[:-n]\n",
    "    rotated=A[-n:]\n",
    "    #print (remain, rotated)\n",
    "    return rotated+remain"
   ]
  },
