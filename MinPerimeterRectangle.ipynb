    "def solution (N):\n",
    "    i=int(N**0.5)\n",
    "    while i>0:\n",
    "        if N%i==0:\n",
    "            break\n",
    "        else:\n",
    "            i-=1\n",
    "    print (i, N/i)\n",
    "    perimeter=int(2*(i+N/i))\n",
    "    return perimeter\n"
   ]
